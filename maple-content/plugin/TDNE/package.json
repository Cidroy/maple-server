{
    "name": "TDNE",
    "version": "1.0",
    "description": "Tour de North East",
    "author": "Rinzler",
    "namespace": "tdne\/core",
    "Maple": {
        "ID": "9c8b3f02777fa69868808d20a00d8358",
        "Name": "Docs",
        "Template": "docs",
        "Path": "%ROOT%%PLG%TDNE",
        "Active": false,
        "Setup": {
            "Path": "setup.php",
			"Backup" : true,
            "Activate": "TDNE\\Setup::Activate",
            "Deactivate": "TDNE\\Setup::Deactivate"
        },
        "Autoload": {
            "TDNE\\ALoc": "class-locations-admin.php",
            "TDNE\\AHub": "class-hub-admin.php",
            "TDNE\\Hub": "class-hub.php",
            "TDNE\\AGame": "class-game-admin.php",
            "TDNE\\Game": "class-game.php"
        },
        "template": "template",
        "app-route": [
            "route-game.json",
            "route-game-admin.json",
            "route-hub.json",
            "route-hub-admin.json",
            "route-location-admin.json"
        ],
        "Shortcode": [],
        "Filter": [],
        "Admin": {
            "Dashboard": [],
            "Plugin": [
                {
                    "Name": "Game",
                    "Bind": "game",
                    "icon": "maps",
                    "To": "TDNE\\AGame::show",
                    "Permission": "tdne-game-admin-area",
                    "More": [
                        {
                            "Name": "Create Task",
                            "Bind": "create-task",
                            "To": "TDNE\\AGame::show_create_task",
                            "Permission": "tdne-create-task"
                        },
                        {
                            "Name": "View Tasks",
                            "Bind": "view-task",
                            "To": "TDNE\\AGame::show_view_task",
                            "Permission": "tdne-create-task"
                        },
                        {
                            "Name": "Edit Tasks",
                            "Bind": "edit-task",
                            "To": "TDNE\\AGame::show_edit_task",
                            "Permission": "tdne-create-task"
                        },
                        {
                            "Name": "Create Event",
                            "Bind": "create-event",
                            "To": "TDNE\\AGame::show_create_event",
                            "Permission": "tdne-create-event"
                        },
                        {
                            "Name": "View Events",
                            "Bind": "view-event",
                            "To": "TDNE\\AGame::show_view_event",
                            "Permission": "tdne-create-event"
                        },
                        {
                            "Name": "Edit Events",
                            "Bind": "edit-event",
                            "To": "TDNE\\AGame::show_edit_event",
                            "Permission": "tdne-create-event"
                        }
                    ]
                },
                {
                    "Name": "Hub",
                    "Bind": "hub",
                    "To": "TDNE\\AHub::show",
                    "icon": "location",
                    "Permission": "tdne-hub-admin-area",
                    "More": [
                        {
                            "Name": "Assign",
                            "Bind": "assign",
                            "To": "TDNE\\AHub::show_assign",
                            "Permission": "tdne-hub-assign"
                        },
                        {
                            "Name": "Edit Assignment",
                            "Bind": "edit",
                            "To": "TDNE\\AHub::show_edit",
                            "Permission": "tdne-hub-assign-edit"
                        }
                    ]
                },
                {
                    "Name": "Places",
                    "Bind": "places",
                    "To": "TDNE\\ALoc::show",
                    "icon": "location",
                    "Permission": "tdne-admin-locations",
                    "More": [
                        {
                            "Name": "Add",
                            "Bind": "add",
                            "To": "TDNE\\ALoc::show_add",
                            "Permission": "tdne-add-location"
                        },
                        {
                            "Name": "Edit",
                            "Bind": "edit",
                            "To": "TDNE\\ALoc::show_edit",
                            "Permission": "tdne-edit-location"
                        }
                    ]
                }
            ]
        }
    },
    "AJAX": {
        "add-task": {
            "function": "TDNE\\AGame::h_add_task",
            "permission": "tdne-create-task",
            "extention": "json"
        },
        "edit-task": {
            "function": "TDNE\\AGame::h_edit_task",
            "permission": "tdne-edit-task",
            "extention": "json"
        },
        "delete-task": {
            "function": "TDNE\\AGame::h_delete_task",
